 $scope.getNext = function(trackListPage) {
    var nextPage;
    console.log(trackListPage);
    if(trackListPage.next == null) {
      return null;
    }
    console.log(trackListPage.next);
    $http.get(trackListPage.next).success(function(response) {
      nextPage = response.tracks;
      console.log(nextPage);
    });
    return nextPage;
  };

